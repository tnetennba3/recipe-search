{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["prettier", "react"],
  "rules": { "prettier/prettier": "error" },
  "parser": "babel-eslint",
  "env": { "es6": true, "browser": true },
  "settings": { "react": { "version": "detect" } }
}
